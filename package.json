{
  "name": "react-native-rename",
  "version": "3.0.0-alpha",
  "description": "Rename react-native app with just one command",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "relink": "npm unlink && npm run prepublish && npm link",
    "format": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint '**/*.{js,jsx}'",
    "react-native-rename": "babel-node src/index.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "format",
      "lint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escolarea/react-native-rename.git#v3.0.0-alpha"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "rename",
    "renamer",
    "react",
    "space"
  ],
  "author": "June Domingo <junedomingo@gmail.com>",
  "license": "MIT",
  "bin": {
    "react-native-rename": "lib/index.js"
  },
  "bugs": {
    "url": "https://github.com/junedomingo/react-native-rename/issues"
  },
  "homepage": "https://github.com/junedomingo/react-native-rename#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "globby": "^11.0.1",
    "node-replace": "^0.3.3",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-eslint": "^8.0.1",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-watch": "^3.1.3",
    "husky": "^0.14.3",
    "jest": "^27.0.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4"
  }
}
